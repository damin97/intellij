<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.damin.shoppingmall.repository.OrderMapper">

    <insert id="insertOrder" parameterType="com.damin.shoppingmall.model.Orders">
        INSERT INTO orders (member_id, prod_name, count)
        VALUES (#{memberId}, #{prodName}, #{count})
    </insert>
    
</mapper>